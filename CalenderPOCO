package com.example.demo;

public class CalenderPOCO {
    public long id;
    public String name;
    public int day;
    public int month;
    public int length;

    public CalenderPOCO(long id, String name, int day, int month, int length) {
        this.id = id;
        this.name = name;
        this.day = day;
        this.month = month;
        this.length = length;
    }

    public void setId(long id) {
        this.id = id;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void setDay(int day) {
        this.day = day;
    }

    public void setMonth(int month) {
        this.month = month;
    }

    public void setLength(int length) {
        this.length = length;
    }

    public CalenderPOCO() {
    }

    public long getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public int getDay() {
        return day;
    }

    public int getMonth() {
        return month;
    }

    public int getLength() {
        return length;
    }
}


